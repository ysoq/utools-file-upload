#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/qrcode@1.5.4/node_modules:/mnt/c/workspace/my/3.utools-插件/文件传输/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/qrcode" "$@"
else
  exec node  "$basedir/../.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/qrcode" "$@"
fi
